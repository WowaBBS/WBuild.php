<?  $Loader->Parent_Class('/Object');    class C_Builder_Exec extends C_Object  {    Var $Descr=[];    Var $List=[];        Function Descr() // VarArgs    {      $this->Descr=Implode(func_get_args());    }        Function ToString()    {      return Implode(' ', $this->List);    }        Function Exec($Cmd, $Args)    {      $this->List[]=$Cmd.' '.$Args->ToString();      //TODO: $Args->CheckObjects      //TODO: $Args->CheckFiles      //TODO: $Args->CheckResultFiles    }    Var $CheckObjects     =[];    Var $CheckFiles       =[];    Var $CheckResultFiles =[];        Function MkDir($v)    {      $v->MkDir();    }        Function CheckObject($v)    {      if(false && IsSet($v->Path))        Echo $v->GetMD5(), ' ',$v->Path , "\n";      $this->CheckObjects[]=$v;    }        Function CheckFile($v)    {      if(false)        Echo $v->GetMD5(), ' ',$v->Path , "\n";      $this->CheckFiles[]=$v->GetFileName();    }        Function CheckResultFile($v)    {      $v->InvalidateCache();      $this->CheckResultFiles[]=$v->GetFileName();    }        Function CheckFiles($l)    {      ForEach($l As $l)        $this->CheckFile($l);    }  };  $DebM=$Loader->Get_Singleton('/Debug/Manager');  $DebM->AddNoInlineClass('C_Builder_Exec');?>