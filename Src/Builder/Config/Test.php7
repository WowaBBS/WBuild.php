<?  Require_Once '../../Using.php';    Function Test($Ctx, $Config, $Check=[])  {    $Loader=$Ctx['Loader'];    $Deb=$Ctx['Deb'];    $C=$Loader->Create_Object('/Builder/Config/Set', ['Config'=>$Config]);    $Deb->Debug($Config);    $Deb->Debug($C->GetContextId());    if($Check)    {       $Defines=$Loader->Get_Singleton('/Builder/Config/Defines');       $Check=$Defines->ValidateSet($Check);       $Deb->Debug($Check);       $Deb->Debug($C->Check($Check));    }    $Deb->WriteLn('----------');    $C->Done();  }  $Deb=$Loader->Get_Singleton('/Debug/Manager')->Get('Test.log');    $Ctx=[    'Loader'=>$Loader ,    'Deb'   =>$Deb    ,  ];    Test($Ctx, []);  Test($Ctx, ['Win32']);  Test($Ctx, ['VC'=>5]);  Test($Ctx, ['VC']);  Test($Ctx, ['Win'=>2, 'Win32']);  Test($Ctx, ['Win32', 'Win'=>2]);  Test($Ctx, ['Win32', 'VC'=>-2017], ['VC']);?>