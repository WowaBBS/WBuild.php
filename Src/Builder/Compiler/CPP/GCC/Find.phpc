<?  class C_Builder_Compiler_CPP_GCC_Find  {    var $Regs=[      'HKLM\SOFTWARE\Microsoft\VisualStudio\SxS\VS7',      'HKCU\SOFTWARE\Microsoft\VisualStudio\SxS\VS7',      'HKLM\SOFTWARE\Wow6432Node\Microsoft\VisualStudio\SxS\VS7',      'HKCU\SOFTWARE\Wow6432Node\Microsoft\VisualStudio\SxS\VS7',    ];    function Find()    {      $Results=[];      ForEach($this->Regs as $Reg)      {        $Cmd='reg query "'.$Reg.'" /t REG_SZ 2>nul';        $Output=[];        $Return=0;        $Res=exec($Cmd, $Output, $Return);        ForEach($Output as $Str)          if(preg_match('/^ +(\d+)\.(\d+) +REG_SZ +(.*)$/', $Str, $Match)===1)          {            $Ver  =$Match[1];            $Ver2 =$Match[2];            $Path =$Match[3];            $Results[$Path]=['Path'=>$Path, 'RegVer'=>[$Ver, $Ver2]];          }      }      return $Results;    }  }?>