<?  $Loader->Parent_Class('/Builder/Asset/FS/File');  class C_Builder_Asset_Cpp_Lib extends C_Builder_Asset_FS_File  {    Var $FileDepends=[];  //Var $Name=''; // TODO:    Protected Function _GetSearchPathClass()    {      return ['Cpp/LibPath'];    }    Protected Function _Init(Array $Args)    {      Parent::_Init($Args);      if($Dlls=$Args['Dlls']??[])      {        $Dlls=$this->CreateAssets('Bin/Dll', $Dlls);        $this->AddFileDepends($Dlls);        ForEach($Dlls As $Dll)          $Dll->Remove();      }    }    Static Function _AddItems(Array &$Res, Array $Args, Array $Paths)    {      $Libs=[];      $Dlls=[];      static $CheckDll=[        '>dll'=>1,      ];          $zDll=false;      ForEach($Paths As $Path)        if(IsSet($CheckDll[$Path]))          $zDll=true;        elseif($zDll)        {          $Dlls[]=$Path;          $zDll=false;        }        else        {          $Args['Dlls']=$Dlls;          Static::_AddItem($Res, $Args, $Path);        }    }      };?>