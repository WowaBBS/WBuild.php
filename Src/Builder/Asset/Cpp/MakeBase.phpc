<?  $Loader->Parent_Class('/Builder/Asset/Inline');  class C_Builder_Asset_Cpp_MakeBase extends C_Builder_Asset_Inline  {    Static Function _ParseParams($Args)    {      $Outer=$Args['Outer'];      $Param=$Args['Param'];      $Compiler=$Outer->RequireAsset('Cpp/Compiler')->Instance;      if(!$Compiler)      {        Var_Dump($Outer);        throw new Exception('Haven`t any compiler');      }      $Param['OutPath']=$Compiler->CreateOutPath($Outer);      Static::Make($Outer, $Compiler, $Param);      return [];    }        Static Function _GetName($Param, $Outer)    {      if(IsSet($Param['Name']))        return $Param['Name'];      if($Asset=$Outer->GetLastAsset('Cpp/Src'))        return $Asset->GetFileNick();      if($Asset=$Outer->GetLastAsset('Cpp/Obj'))        return $Asset->GetFileNick();      return 'Unknown';    }        Static Function Make($Outer, $Compiler, $Param)    {    }  };?>