<?  $Loader->Parent_Class('/Builder/Asset/Inline');  class C_Builder_Asset_Cpp_MakeBase extends C_Builder_Asset_Inline  {    Function _ParseParams($Args)    {      $Outer=$Args['Outer'];      $Param=$Args['Param'];      $Compiler=$Outer->RequireAsset('Cpp/Compiler');      $Param['OutPath']=$Compiler->Instance->CreateOutPath($Outer);      if($Compiler!==null)        $this->Make($Outer, $Compiler->Instance, $Param);      else      {        Var_Dump($Outer);        throw new Exception('Haven`t any compiler');      }      return [];    }        Function _GetName($Outer)    {      if($Asset=$Outer->GetLastAsset('Cpp/Src'))        return $Asset->GetFileNic();      if($Asset=$Outer->GetLastAsset('Cpp/Obj'))        return $Asset->GetFileNic();      return 'Unknown';    }        Function Make($Outer, $Compiler, $Param)    {    }  };?>