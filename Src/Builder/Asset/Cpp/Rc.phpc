<?  $Loader->Parent_Class('/Builder/Asset/FS/File');  class C_Builder_Asset_Cpp_Rc extends C_Builder_Asset_FS_File  {    Protected Function _Init(Array $Args)    {      Parent::_Init($Args);      Static::MakeRc($this->Outer, $this);    }        Protected Function _GetSearchPathClass()    {      return [];    }    Static Function MakeRc($Outer, $RcAsset)    {      $Compiler=$Outer->RequireAsset('Cpp/Compiler')->Instance;      $EnumCache=$Outer->GetModuleInstance()->EnumCache;      [$Dir, $Name]=$RcAsset->GetDirAndNick();      $Num=$EnumCache->AllocNum($Dir, 'Rc', ['Pad'=>1]);      $ResNick=$Num.'/'.$Name;      $OutPath=$Compiler->CreateOutPath($Outer);      $ResPath=$OutPath.'/Rc/'.$ResNick.'.res';      $ResAsset=$Outer->CreateAsset('Cpp/Res', ['Path'=>$ResPath, '>Create'=>1]);      $Compiler->BuildRc([        'Outer'    =>$Outer,        'Exec'     =>$Outer->CreateExec(),        'RcAsset'  =>$RcAsset,        'ResAsset' =>$ResAsset,        'OutPath'  =>[$OutPath, $ResNick],      ]);    }  };?>