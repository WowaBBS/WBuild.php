<?  $Loader->Parent_Class('/Builder/Asset/FS/File');  class C_Builder_Asset_Cpp_ResX extends C_Builder_Asset_FS_File  {    Protected Function _Init(Array $Args)    {      Parent::_Init($Args);      Static::MakeResX($this->Outer, $this);    }        Protected Function _GetSearchPathClass()    {      return [];    }    Static Function MakeResX($Outer, $ResXAsset)    {      $Compiler=$Outer->RequireAsset('Cpp/Compiler')->Instance;      $EnumCache=$Outer->GetModuleInstance()->EnumCache;      [$Dir, $Name]=$ResXAsset->GetDirAndNick();      $Num=$EnumCache->AllocNum($Dir, 'ResX', ['Pad'=>1]);      $ResNick=$Num.'/'.$Name;      $OutPath=$Compiler->CreateOutPath($Outer);      $ResPath=$OutPath.'/ResX/'.$ResNick.'.resources';      $Resources=$Outer->CreateAsset('Cpp/Resources', ['Path'=>$ResPath, '>Create'=>1]);      $Compiler->BuildResX([        'Outer'     =>$Outer,        'Exec'      =>$Outer->CreateExec(),        'Using'     =>$Outer->GetAssets    ('Cpp/Using'     ),        'ResXAsset' =>$ResXAsset,        'Resources' =>$Resources,        'OutPath'   =>[$OutPath, $ResNick],      ]);    }  };?>