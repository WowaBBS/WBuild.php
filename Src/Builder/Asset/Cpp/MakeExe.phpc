<?  $Loader->Parent_Class('/Builder/Asset/Cpp/Compile');  class C_Builder_Asset_Cpp_MakeExe extends C_Builder_Asset_Cpp_Compile  {    Function Make($Outer, $Compiler, $Param)    {      $ExeName=$this->_GetName($Outer);          $this->Compile($Outer, $Compiler, $Param);            $Compiler->BuildExe([        'Exec'     =>$Outer->CreateExec(),        'Objects'  =>$Outer->RemoveAssets ('Cpp/Obj'),        'Libs'     =>$Outer->GetAssetsR   ('Cpp/Lib'),        'ExeAsset' =>$Outer->CreateAsset  ('Bin/Exe', ['Path'=>$Compiler->GetExeFile($ExeName)]),      ]);    }  };?>