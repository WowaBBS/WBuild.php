<?  $Loader->Parent_Class('/Builder/Asset/Cpp/Compile');  class C_Builder_Asset_Cpp_MakeExe extends C_Builder_Asset_Cpp_Compile  {    Static Function Make($Outer, $Compiler, $Param)    {      $ExeName=Static::_GetName($Outer);      $OutPath=$Param['OutPath'];          Static::Compile($Outer, $Compiler, $Param);      $PathExe=$OutPath.'Bin';      $PathExeType='Bin/Path'    ;      $PathExeAsset=$Outer->CreateAsset($PathExeType, $PathExe);            $Libs=$Outer->GetAssetsR   ('Cpp/Lib');      $FileDepends=[];      if(IsSet($Param['>FileDepends']))        $FileDepends[]=$Param['>FileDepends'];      ForEach($Libs As $Lib)        $FileDepends[]=$Lib->FileDepends;               $Compiler->BuildExe([        'Exec'     =>$Outer->CreateExec(),        'Objects'  =>$Outer->RemoveAssets ('Cpp/Obj'),        'Libs'     =>$Libs,        'ExeAsset' =>$Outer->CreateAsset  ('Bin/Exe', ['Path'=>$Compiler->GetExeFile($ExeName), '>Create'=>1, '>FileDepends'=>$FileDepends]),      ]);      $PathExeAsset->Remove();    }  };?>