<?  $Loader->Parent_Class('/Builder/Asset/Cpp/Compile');  class C_Builder_Asset_Cpp_MakeExe extends C_Builder_Asset_Cpp_Compile  {    Static Function Make($Outer, $Compiler, $Param)    {      $ExeName=Static::_GetName($Param, $Outer);      $OutPath=$Param['OutPath'];          Static::Compile($Outer, $Compiler, $Param);      $PathExe=$OutPath.'Bin';      $PathExeType='Bin/Path'    ;      $PathExeAsset=$Outer->CreateAsset($PathExeType, $PathExe);            $Compiler->BuildExe([        'Outer'     =>$Outer,        'Exec'      =>$Outer->CreateExec(),        'Using'     =>$Outer->GetAssets    ('Cpp/Using'     ),        'Objects'   =>$Outer->RemoveAssets ('Cpp/Obj'       ),        'Libs'      =>$Outer->GetAssetsR   ('Cpp/Lib'       ),        'ResAsset'  =>$Outer->RemoveAssets ('Cpp/Res'       ),        'Resources' =>$Outer->RemoveAssets ('Cpp/Resources' ),        'ExeAsset'  =>$Outer->CreateAsset  ('Bin/Exe', [          'Path'=>$Compiler->GetExeFile($ExeName),          '>FileDepends'=>$Param['>FileDepends']??[],          '>Create'=>1,        ]),        'OutPath'  =>[$OutPath, $ExeName],      ]);      $PathExeAsset->Remove();    }  };?>