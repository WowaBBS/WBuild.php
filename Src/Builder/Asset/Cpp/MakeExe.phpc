<?  $Loader->Parent_Class('/Builder/Asset/Cpp/Compile');  class C_Builder_Asset_Cpp_MakeExe extends C_Builder_Asset_Cpp_Compile  {    Function Make($Outer, $Compiler, $Param)    {      $ExeName=$this->_GetName($Outer);      $OutPath=$Param['OutPath'];          $PathObj=$OutPath.'Obj';      $PathLib=$OutPath.'Lib';      $PathExe=$OutPath.'Bin';            $PathObjType='Cpp/ObjPath' ;      $PathExeType='Bin/Path'    ;      $PathObjAsset=$Outer->CreateAsset($PathObjType, $PathObj);      $PathExeAsset=$Outer->CreateAsset($PathExeType, $PathExe);            $this->Compile($Outer, $Compiler, $Param);            $Compiler->BuildExe([        'Exec'     =>$Outer->CreateExec(),        'Objects'  =>$Outer->RemoveAssets ('Cpp/Obj'),        'Libs'     =>$Outer->GetAssetsR   ('Cpp/Lib'),        'ExeAsset' =>$Outer->CreateAsset  ('Bin/Exe', $Compiler->GetExeFile($ExeName)),      ]);      $PathObjAsset->Remove();      $PathExeAsset->Remove();    }  };?>