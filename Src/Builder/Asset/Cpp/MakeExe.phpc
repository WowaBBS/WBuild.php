<?  $Loader->Parent_Class('/Builder/Asset/Cpp/Compile');  class C_Builder_Asset_Cpp_MakeExe extends C_Builder_Asset_Cpp_Compile  {    Static Function Make($Outer, $Compiler, $Param)    {      $ExeName=Static::_GetName($Param, $Outer);      $OutPath=$Param['OutPath'];          Static::Compile($Outer, $Compiler, $Param);      $PathExeAsset=$Outer->CreateAsset('Bin/Path', $OutPath.'Bin');            $Compiler->BuildExe([        'Outer'     =>$Outer,        'Name'      =>$ExeName,        'Exec'      =>$Outer->CreateExec([$OutPath, $ExeName]),        'Using'     =>$Outer->GetAssets    ('Cpp/Using'     ),        'Objects'   =>$Outer->RemoveAssets ('Cpp/Obj'       ),        'Libs'      =>$Outer->GetAssets    ('Cpp/Lib'       ),        'ResAsset'  =>$Outer->RemoveAssets ('Cpp/Res'       ),        'Resources' =>$Outer->RemoveAssets ('Cpp/Resources' ),        'Manifests' =>$Outer->RemoveAssets ('Cpp/Manifest'  ),        'ExeAsset'  =>$Outer->CreateAsset  ('Bin/Exe', [          'Path'=>$Compiler->GetExeFile($ExeName),          '>FileDepends'=>$Param['>FileDepends']??[],          '>Create'=>1,        ]),        'OutPath'  =>[$OutPath, $ExeName],      ]);      $PathExeAsset->Remove();    }  };?>