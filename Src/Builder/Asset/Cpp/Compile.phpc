<?  $Loader->Parent_Class('/Builder/Asset/Cpp/MakeBase');  class C_Builder_Asset_Cpp_Compile extends C_Builder_Asset_Cpp_MakeBase  {    Function Make($Outer, $Compiler, $Param)    {      Parent::Make($Outer, $Compiler, $Param);      $this->Compile($Outer, $Compiler, $Param);    }        Function Compile($Outer, $Compiler, $Param)    {      $Items=[];      ForEach($Outer->RemoveAssets('Cpp/Src') As $SrcAsset)      {        $ObjFile=$Compiler->GetObjFile($SrcAsset);        $ObjAsset=$Outer->CreateAsset('Cpp/Obj', ['Path'=>$ObjFile]);        $Exec=$ObjAsset->CreateExec();        $ObjAsset->AddTask($Exec);                $Items[]=[          'SrcAsset' =>$SrcAsset ,          'ObjAsset' =>$ObjAsset ,          'Exec'     =>$Exec     ,        ];      }      $Compiler->Compile([        'Defines'  =>$Outer->GetAssets('Cpp/Define'  ),        'Includes' =>$Outer->GetAssets('Cpp/Include' ),        'Items'    =>$Items,      ]);    }  };?>