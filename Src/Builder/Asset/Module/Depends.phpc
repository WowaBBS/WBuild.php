<?  $Loader->Parent_Class('/Builder/Asset/Inline');  class C_Builder_Asset_Module_Depends extends C_Builder_Asset_Inline  {    Static Function _ParseParams($Args)    {      $Outer=$Args['Outer'];      $Param=$Args['Param'];      $I=$Outer->GetModuleInstance();      $M=$I->GetModules();      ForEach($Param As $ModuleName)      {        $MI=$M->Get($ModuleName)->CreateInstance($I->Context);        $I->Joined[$ModuleName]=$MI;        $I->AddFrom($MI, null, IsSet($Args['Debug']['AddFrom']));      }      return [];    }  };?>