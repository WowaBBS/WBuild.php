<?  $Loader->Parent_Class('/Builder/Asset/List');  class C_Builder_Asset_Module_Instance extends C_Builder_Asset_List  {    Var $Context   ;    Var $Module    ;    Var $Modules   ;    Var $EnumCache ;        Protected Function _Init(Array $Args)    {      $this->Context =$Args['Context' ];      $this->Module  =$Args['Module'  ];      $this->Modules =$Args['Modules' ];      $Key=Serialize($this->Context->GetContextId());    //Echo 'MD5_Storage ', $this->Module->MD5_Storage ->FullPath, "\n";    //Echo 'EnumCache   ', $this->Module->EnumCache   ->FullPath, "\n";      $this->EnumCache = $this->Module->EnumCache->AllocEnumDir($Key, 'Instance');    //Echo 'EnumCacheI  ', $this->EnumCache   ->FullPath, "\n";      parent::_Init($Args);    }        Function IsInstance() { return true; }        Function Make()    {      $Res=[];      $Depends=[];      ForEach($this->Public As $List)        ForEach($List As $Item)          $Item->GetMakeTasks($Res, $Depends);      return $Res;    }    Function GetModuleInstance()    {      return $this;    }        Function GetModules()    {      return $this->Modules;    }        Function GetMD5Cache($FullPath)    {      return $this->Module->GetMD5Cache($FullPath);    }  // gerald+donald=robert  };?>