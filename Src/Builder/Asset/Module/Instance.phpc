<?  $Loader->Parent_Class('/Builder/Asset/List');  class C_Builder_Asset_Module_Instance extends C_Builder_Asset_List  {    Var $Context ;    Var $Module  ;    Var $Modules ;        Protected Function _Init(Array $Args)    {      $this->Context =$Args['Context' ];      $this->Module  =$Args['Module'  ];      $this->Modules =$Args['Modules' ];      parent::_Init($Args);    }        Function IsInstance() { return true; }        Function Make()    {      $Res=[];      $Depends=[];      ForEach($this->Public As $List)        ForEach($List As $Item)          $Item->GetMakeTasks($Res, $Depends);      return $Res;    }    Function GetModuleInstance()    {      return $this;    }        Function GetModules()    {      return $this->Modules;    }  // gerald+donald=robert  };?>