<?  $Loader->Parent_Class('/Builder/Asset/Base');  $Loader->Using_Module('/Array');  class C_Builder_Asset_Exec_Option extends C_Builder_Asset_Base  {  //Var $Program ='';    Var $Options =[];  //Function GetKey() { return $this->Key; }        Function _InitParam(Array &$Param)    {    //$this->Program=Array_Shift_Checked($Param);      $Options=[];      for($i=0; IsSet($Param[$i]); $i++)      {        $Options[]=$Param[$i];        UnSet($Param[$i]);      }      $this->Options=$Options;    }    Function AddArgsTo($Args)    {      ForEach($this->Options As $Option)        $Args->Add($Option);    }        Function IsOverride() { return false; }  };  $DebM=$Loader->Get_Singleton('/Debug/Manager');  $DebM->AddInlineClass('C_Builder_Asset_Exec_Option');?>