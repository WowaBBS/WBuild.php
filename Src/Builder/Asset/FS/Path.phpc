<?  $Loader->Parent_Class('/Builder/Asset/FS/PathBase');  class C_Builder_Asset_FS_Path extends C_Builder_Asset_FS_PathBase  {    Function GetSubPath($Path)    {      $Parent=$this->GetPath();      $l=StrLen($Parent);      if($l && SubStr($Parent, $l-1, 1)==='/')        return $Parent.$Path;      return $Parent.'/'.$Path;    }        Function Search($Path, $bIsFile=true)    {      $Path=$this->GetSubPath($Path);      if(!$bIsFile && Is_Dir($Path) || $bIsFile && Is_File($Path))        return $Path;      return '';    }        Function SearchDir  ($Path) { return Search($Path, false ); }    Function SearchFile ($Path) { return Search($Path, true  ); }        Protected Function _IsFile() { return false; }  };?>