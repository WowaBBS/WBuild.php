<?  $Loader->Parent_Class('/Builder/Asset/Inline');  class C_Builder_Asset_Depends extends C_Builder_Asset_Inline  {    Function _ParseParams($Args)    {      $Outer=$Args['Outer'];      $Param=$Args['Param'];      $I=$Outer->GetModuleInstance();      $M=$I->GetModules();      ForEach($Param As $Name)      {        $MI=$M->Get($Name)->CreateInstance($I->Context);        $I->AddFrom($MI, null, IsSet($Args['Debug']['AddFrom']));      }      return [];    }  };?>