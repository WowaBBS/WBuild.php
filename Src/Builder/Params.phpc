<?  $Loader->Parent_Class('/Object');    class C_Builder_Params extends C_Object  {    Var $List=[];    Protected Function _Cloneable() { return true; }        Function Add() // VarArgs    {      $this->List[]=Implode(func_get_args());    }        Function ToString()    {      return Implode(' ', $this->List);    }        Var $CheckObjects     =[];    Var $CheckFiles       =[];    Var $CheckResultFiles =[];        Function CheckObject($v)    {      if(false && IsSet($v->Path))        Echo $v->GetMD5(), ' ',$v->Path , "\n";      $this->CheckObjects[]=$v;    }        Function CheckFile($v)    {      if(false)        Echo $v->GetMD5(), ' ',$v->Path , "\n";      $this->CheckFiles[]=$v;    }        Function MkDir($v)    {      $v->MkDir();    }        Function CheckResultFile($v)    {      $v->InvalidateCache();      $this->CheckResultFiles[]=$v;    }    Function CheckFiles($l)    {      ForEach($l As $l)        $this->CheckFile($l);    }  };?>