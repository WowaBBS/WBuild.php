<?  $Loader->Parent_Class('/Object');    class C_Builder_Params extends C_Object  {    Var $List=[];    Protected Function _Clonable() { return true; }        Function Add() // VarArgs    {      $this->List[]=Implode(func_get_args());    }        Function ToString()    {      return Implode(' ', $this->List);    }        Var $CheckObjects     =[];    Var $CheckFiles       =[];    Var $CheckResultFiles =[];        Function CheckObject($v)    {      $this->CheckObjects[]=$v;    }        Function CheckFile($v)    {      $this->CheckFiles[]=$v;    }        Function CheckResultFile($v)    {      $this->CheckResultFiles[]=$v;    }    Function CheckFiles($l)    {      ForEach($l As $l)        $this->CheckFile($l);    }  };?>