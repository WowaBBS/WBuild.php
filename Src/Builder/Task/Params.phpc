<?  $Loader->Parent_Class('/Object');    class C_Builder_Task_Params extends C_Object  {    Var $List=[];    Var $bInline=false;    Protected Function _Cloneable() { return true; }        Function Add() // VarArgs    {      $this->List[]=Implode(func_get_args());    }        Function Prepare($Exec)    {      if($this->bInline)        return;      $Exec->Exec('');      $Exec->Exec('Set PARAMS=');      ForEach($this->List As $Item)        $Exec->Exec('Set PARAMS=%PARAMS% '.$Item);    }        Function ToString()    {      if($this->bInline)        return Implode(' ', $this->List);      return '%PARAMS%';    }  };?>