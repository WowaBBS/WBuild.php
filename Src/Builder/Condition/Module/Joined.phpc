<?  $Loader->Parent_Class('/Builder/Condition/Base');    class C_Builder_Condition_Module_Joined extends C_Builder_Condition_Base  {     Var $List=[];          function _Init(Array $Args)     {       parent::_Init($Args);       // TODO: Check list of string       $this->List=$Args['Param'];     }          private function CheckModuleInstance($v, $Args)     {       $Joined=$v->Joined;       ForEach($this->List As $Name)         if(!IsSet($Joined[$Name]))           return false;       return true;     }       }?>