<? $Loader->Parent_Class('/Builder/Condition/Base'); $Loader->Using_Class('/Builder/Config/Defines'); class C_Builder_Condition_Config extends C_Builder_Condition_Base {   Var $Values=[];    function _Init(Array $Args)   {     parent::_Init($Args);     $OS_Defines=$this->Get_Singleton('/Builder/Config/Defines');     $this->Values=$OS_Defines->ValidateSet($Args['Param']);   //$OS_Defines->Done();          if(IsSet($Args['Debug']['If']))     {       $Deb=$this->Get_Singleton('/Debug/Manager')->Get('Condition.log');       $Deb->WriteLn('Compile condition:');       $Deb->Debug($Args);       $Deb->WriteLn('  Into:');       $Deb->Debug($this->Values);     }   }      function CheckContext(C_Builder_Context $Context, $Args)   {     if(IsSet($Args['Debug']['If']))     {       $Deb=$this->Get_Singleton('/Debug/Manager')->Get('Condition.log');       $Deb->WriteLn('Check condition:');       $Deb->Debug($this->Values);       $Deb->WriteLn('  With:');       $Deb->Debug($Context->Config->Value);     }     return $Context->Config->Check($this->Values);   } }?>