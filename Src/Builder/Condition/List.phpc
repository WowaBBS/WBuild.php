<?  $Loader->Parent_Class('/Builder/Condition/Base');    class C_Builder_Condition_List extends C_Builder_Condition_Base  {    Var $List=[];        function _Init(Array $Args)    {      parent::_Init($Args);      $List=&$this->List;      $Param=$Args['Param'];      ForEach($Param As $Item)        $List[]=$this->Create($Item);    }        function CheckAsset($Asset, $Args)    {      ForEach($this->List As $Item)        if(!$Item->CheckAsset($Asset, $Args))          return false;      return True;    }  }?>