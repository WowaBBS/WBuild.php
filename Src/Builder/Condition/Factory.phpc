<?  $Loader->Parent_Class('/Object');    class C_Builder_Condition_Factory extends C_Object  {    Var $List=[];      function Create($Args)    {          if(!Is_Array($Args))        $Args=['Error', $Args];      if(!IsSet($Args[0]))        $Args=['Error', $Args];      $Type=$Args[0];      if(Is_Array($Type))        $Type='List';      if(!Is_String($Type))      {        $Args=['Error', $Args];        $Type='Error';      }      Array_Shift($Args);      $List=&$this->List;      if(!IsSet($List[$Type]))        $List[$Type]='/Builder/Condition/'.$Type;      $Res=$this->Create_Object($List[$Type], ['Param'=>$Args, 'Factory'=>$this]);      if(!$Res)      {        Echo '[Error] Condition ',$Type,' is not exists', "\n";        $Type='Error';        $Res=$this->Create("Condittion not exists");      }      return $Res;    }  }?>